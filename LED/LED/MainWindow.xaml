        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:LED"
        mc:Ignorable="d"
        Title="LED" Height="150.838" Width="350">
    <Grid>
        <Button x:Name="button" Content="Send" HorizontalAlignment="Left" Margin="240,80,0,0" VerticalAlignment="Top" Width="75" Click="Button_Click"/>
